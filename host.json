{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Error",
      "Function": "Information",
      "Host.Results": "Error",
      "Host.Aggregator": "Error",
      "Host.General": "Error",
      "Host.Startup": "Error",
      "Host.Executor": "Error",
      "Host.Triggers": "Error",
      "Microsoft": "Error",
      "Azure.Storage": "Error"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}
